{
  "name": "@q-free/ui",
  "version": "2.1.18",
  "description": "Q-free design system library",
  "main": "./dist/js/bundle.min",
  "types": "dist/src/index",
  "style": "./dist/main.css",
  "keywords": [
    "ui",
    "react",
    "typescript",
    "library",
    "tailwindcss",
    "typedoc"
  ],
  "author": "Stevica Canadi",
  "license": "ISC",
  "scripts": {
    "build": "yarn run tailwind:css && rimraf dist/* && typedoc --json dist/typedoc/api.json ./src --mode file --tsconfig ./tsconfig.json && webpack -p --config=webpack.config.js",
    "build:ts": "webpack -p --config=webpack.config.js",
    "tailwind:css": "node twObjGen.js && tailwind build tailwind.css -c tailwind.config.js -o src/index.css",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "typedoc": "rimraf dist/typedoc/ && typedoc --excludeNotExported --json dist/typedoc/api.json ./src --mode file --tsconfig ./tsconfig.json",
    "kill": "taskkill /im node.exe -F",
    "docz:dev": "docz dev",
    "postbuild": "cpy './tailwindObject.json' dist/",
    "playground": "cd playground && parcel index.html"
  },
  "dependencies": {
    "@bumaga/tabs": "^0.2.0",
    "@carbon/icons-react": "^10.17.0",
    "@tailwindcss/forms": "^0.3.2",
    "@types/carbon__icons-react": "^10.28.0",
    "autoprefixer": "^10.2.5",
    "dayzed": "^3.1.0",
    "fs": "^0.0.1-security",
    "lodash-es": "^4.17.15",
    "moment": "^2.26.0",
    "postcss": "^8.2.15",
    "react-imask": "^6.0.5",
    "react-scrollbars-custom": "^4.0.25",
    "react-slider": "^1.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/dayzed": "^3.1.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.6",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-slider": "^1.0.0",
    "babel-loader": "^8.1.0",
    "cpy-cli": "^3.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "parcel": "^2.0.0-beta.2",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2",
    "terser-webpack-plugin": "^3.0.3",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.0-3",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
